
public class MeetingTime {
    String day;
    String startHour;
    String finishHour;
    Room room;

    public MeetingTime(String day,String startHour,String finishHour,Room roomCode){
        this.day = day;
        this.startHour = startHour;
        this.finishHour = finishHour;
        this.room = roomCode;
    }

    public MeetingTime(){}

    public String toString(){
        return "Day: " + day + " StartHour: " + startHour + " FinishHour: " + finishHour + "" + room;
    }
}
